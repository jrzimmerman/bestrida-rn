{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "rules": {
    "strict": 0,
    "no-global-assign": 0,
    "no-unsafe-negation": 0,
    "comma-dangle": 0
  }
}
